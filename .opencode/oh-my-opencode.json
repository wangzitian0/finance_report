{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "agents": {
    "sisyphus": {
      "model": "github-copilot/claude-sonnet-4.6",
      "skills": [
        "agents-rules",
        "tools-index",
        "domain/development",
        "domain/schema",
        "domain/accounting",
        "domain/reconciliation",
        "domain/reporting",
        "domain/extraction",
        "domain/infra-operations",
        "domain/secrets-management",
        "domain/github-operations",
        "professional/backend-development"
      ]
    },
    "prometheus": {
      "replace_plan": true,
      "model": "openai/gpt-5.2",
      "skills": ["agents-rules", "tools-index"]
    },
    "oracle": {
      "model": "openai/gpt-5.2",
      "skills": ["agents-rules", "tools-index"]
    },
    "momus": {
      "model": "openai/gpt-5.2",
      "skills": ["agents-rules"]
    },
    "metis": {
      "model": "github-copilot/claude-opus-4.6",
      "skills": ["agents-rules"]
    },
    "hephaestus": {
      "model": "openai/gpt-5.3-codex",
      "skills": ["agents-rules", "tools-index"]
    },
    "atlas": {
      "model": "zai-coding-plan/glm-5",
      "skills": [
        "agents-rules",
        "tools-index",
        "domain/development",
        "domain/schema",
        "domain/accounting",
        "domain/reconciliation",
        "domain/reporting",
        "domain/extraction",
        "domain/infra-operations",
        "domain/secrets-management",
        "domain/github-operations",
        "professional/backend-development"
      ]
    },
    "explore": {
      "model": "zai-coding-plan/glm-4.7",
      "skills": ["agents-rules", "tools-index", "headless-fast-internal-browser"]
    },
    "librarian": {
      "model": "google/antigravity-gemini-3-flash",
      "skills": ["agents-rules", "tools-index", "headless-fast-internal-browser", "headed-internal-browser"]
    },
    "multimodal-looker": {
      "model": "zai-coding-plan/glm-4.6v",
      "skills": ["agents-rules", "tools-index"]
    }
  },
  "categories": {
    "quick": {
      "model": "zai-coding-plan/glm-4.7",
      "description": "Trivial tasks - single file changes, typo fixes"
    },
    "visual-engineering": {
      "model": "google/antigravity-gemini-3-flash",
      "description": "Frontend, UI/UX, design, styling"
    },
    "ultrabrain": {
      "model": "openai/gpt-5.3-codex",
      "description": "Deep logical reasoning, complex architecture decisions"
    },
    "deep": {
      "model": "openai/gpt-5.3-codex",
      "description": "Goal-oriented autonomous problem-solving. Thorough research before action. For hairy problems requiring deep understanding."
    },
    "artistry": {
      "model": "google/antigravity-gemini-3-flash",
      "description": "Complex problem-solving with unconventional, creative approaches - beyond standard patterns"
    },
    "writing": {
      "model": "zai-coding-plan/glm-5",
      "description": "Documentation, prose, technical writing"
    },
    "unspecified-low": {
      "model": "zai-coding-plan/glm-5",
      "description": "Tasks that don't fit other categories, low effort required"
    },
    "unspecified-high": {
      "model": "github-copilot/claude-opus-4.6",
      "description": "Tasks that don't fit other categories, high effort required"
    }
  },
  "skills": {
    "sources": [
      {
        "path": ".opencode/skills",
        "recursive": true
      }
    ]
  },
  "background_task": {
    "defaultConcurrency": 5,
    "staleTimeoutMs": 180000,
    "providerConcurrency": {
      "github-copilot": 3,
      "openai": 5,
      "google": 10,
      "zai-coding-plan": 14
    },
    "modelConcurrency": {
      "github-copilot/claude-opus-4.6": 2,
      "github-copilot/claude-sonnet-4.6": 3,
      "openai/gpt-5.2": 4,
      "openai/gpt-5.3-codex": 3,
      "zai-coding-plan/glm-5": 5,
      "zai-coding-plan/glm-4.7": 10,
      "zai-coding-plan/glm-4.6v": 10,
      "google/antigravity-gemini-3-flash": 8
    }
  },
  "git_master": {
    "commit_footer": true,
    "include_co_authored_by": true
  }
}
